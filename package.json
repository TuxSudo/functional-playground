  "bugs": {
    "url": "https://github.com/TuxSudo/functional-playground/issues"
  },
  "homepage": "https://github.com/TuxSudo/functional-playground",
  "devDependencies": {
    "babel": "^5.1.11",
    "babel-jest": "^5.0.1",
    "jest-cli": "^0.4.0",
    "jshint": "^2.7.0",
    "rimraf": "^2.3.2",
    "watch": "^0.16.0"
  },
  "jest": {
